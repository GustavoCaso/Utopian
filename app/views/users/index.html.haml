.page_wrap
  %table.index_table
    %tr
      %th
        Email
      %th
        Password
      %th
      %th
    - @users.each do |user|
      %tr
        %td= user.email
        %td= user.password
        %td= link_to "Edit", edit_user_path(user)
        %td= link_to "Delete", user_path(user), method: :delete, data: {confirm: "Are you sure ?"}
